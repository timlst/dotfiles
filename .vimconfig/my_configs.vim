" Toggle paste mode with F2
map <F3> :setlocal paste!<cr>

" Comment out with F7
map <F7> gcc

" Open NerdTree with F9
map <F9> ,nn

" Toggle line numbers
map <F2> :setlocal number!<cr>

" Quickly insert an empty new line without entering insert mode
nnoremap <Leader>o o<Esc>
nnoremap <Leader>O O<Esc>
